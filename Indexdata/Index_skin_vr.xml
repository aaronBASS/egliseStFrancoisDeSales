<?xml version="1.0" encoding="UTF-8"?>
<krpano version="1.19">

    
  <!-- Spot Style - ArrowUpPerspective -->


  <style name="IconArrowUpPerspective"
         url="%FIRSTXML%/spots/iconasset_18_user_defaultpoint3.png"
         visible="false"
         edge="center"
         onhover="onhoverIconArrowUpPerspective"
         onover="onoverIconArrowUpPerspective"
         onout="onoutIconArrowUpPerspective"

         height="70"
         width="70"
         />
  <action name="onhoverIconArrowUpPerspective">
    showtooltip('hotspot');
  </action>
  <action name="onoverIconArrowUpPerspective">
tween(scale, 1.2);

pauseautorotation(forcehotspot);

  </action>
  <action name="onoutIconArrowUpPerspective">
    hideTooltip();
    tween(scale, 1);

resumeautorotation(forcehotspot);

  </action>



    
